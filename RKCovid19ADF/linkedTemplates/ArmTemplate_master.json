{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "RKCovid19ADF"
		},
		"ls_ablob_rkcovid19reportingsa_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_ablob_rkcovid19reportingsa'"
		},
		"ls_ablob_rkcovid19reportingsadl_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_ablob_rkcovid19reportingsadl'"
		},
		"ls_ablob_rkcovid19reportingsadl_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://rkcovid19sadl.dfs.core.windows.net"
		},
		"tr_ingest_population_data_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/6934d6bf-9675-4af1-b04f-94837e48b78d/resourceGroups/covid19RG/providers/Microsoft.Storage/storageAccounts/rkcovid19sa"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "RKCovid19ADF_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_ablob_rkcovid19reportingsa_connectionString": {
						"value": "[parameters('ls_ablob_rkcovid19reportingsa_connectionString')]"
					},
					"ls_ablob_rkcovid19reportingsadl_accountKey": {
						"value": "[parameters('ls_ablob_rkcovid19reportingsadl_accountKey')]"
					},
					"ls_ablob_rkcovid19reportingsadl_properties_typeProperties_url": {
						"value": "[parameters('ls_ablob_rkcovid19reportingsadl_properties_typeProperties_url')]"
					},
					"tr_ingest_population_data_properties_typeProperties_scope": {
						"value": "[parameters('tr_ingest_population_data_properties_typeProperties_scope')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}